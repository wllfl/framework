<phpunit
	backupGlobals="false"
	bootstrap="bootstrap.php"
	colors="true"
	debug="true"
	>
	<filter>
		<whitelist>
			<directory>
				src/WllFrame
			</directory>
		</whitelist>
	</filter>
	<logging>
		<log type="coverage-html"
			  target="./log/report"
			  charset="UTF-8"
			  yui="true"
			  highlight="true"
			  lowUpperBound="35"
			  highLowerBound="70"/>
	    <log type="testdox-html" target="./log/testdox.html"/>
	</logging>
</phpunit>